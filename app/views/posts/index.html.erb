<h2>
  みんなのつぶやき
</h2>

<div class="row">
  <div class="col-md-12">
    <table class="table table-dark">
      <thead-class="thead-light">
        <tr>
          <th>ID</th>
          <th>本文</th>
        </tr>
      </thead>
        <tbody>
          <% @posts. each do |post| %>
            <tr>
              <td> <%= post.username %> </td>
              <td> <%= post.body%> </td>
            </tr>
          <% end %>
        </tbody>
    </table>
  </div>
</div>